    {
      "original": "「…………」",
      "lines": [
        22,
        28
      ]
    },
    {
      "original": "「えへへ」",
      "lines": [
        6
      ]
    },
    {
      "original": "「お、うまいなー」",
      "lines": [
        46
      ]
    },
    {
      "original": "「おやすみ、アルル」",
      "lines": [
        75
      ]
    },
    {
      "original": "「おやすみなさい、蓮さん」",
      "lines": [
        69
      ]
    },
    {
      "original": "「かわ……いい……」",
      "lines": [
        40
      ]
    },
    {
      "original": "「このままだとアルルの可愛さに溺れるなって」",
      "lines": [
        39
      ]
    },
    {
      "original": "「ご臨終です♪」",
      "lines": [
        50
      ]
    },
    {
      "original": "「すぅ、すぅ……」",
      "lines": [
        71
      ]
    },
    {
      "original": "「そうですね……」",
      "lines": [
        67
      ]
    },
    {
      "original": "「そうなったら困るなー。一緒に海の藻屑だ」",
      "lines": [
        49
      ]
    },
    {
      "original": "「そうなんですけど、もう、なんていうか」",
      "lines": [
        55
      ]
    },
    {
      "original": "「そっか」",
      "lines": [
        16
      ]
    },
    {
      "original": "「それが幸せ？」",
      "lines": [
        59
      ]
    },
    {
      "original": "「でも、そうですね。まだ私も泳ぎは得意じゃないですし」",
      "lines": [
        45
      ]
    },
    {
      "original": "「なに、アルル？」",
      "lines": [
        7
      ]
    },
    {
      "original": "「なんでもないです♪」",
      "lines": [
        21
      ]
    },
    {
      "original": "「なんでもないです」",
      "lines": [
        8,
        15
      ]
    },
    {
      "original": "「はぁ、もう少し、お話したい、のに……」",
      "lines": [
        65
      ]
    },
    {
      "original": "「はあ……私こんなに幸せでいいのかなあ……」",
      "lines": [
        53
      ]
    },
    {
      "original": "「はいはい？」",
      "lines": [
        20
      ]
    },
    {
      "original": "「まだまだ言いたいことはいっぱいあるんですけど……」",
      "lines": [
        61
      ]
    },
    {
      "original": "「んー？」",
      "lines": [
        33
      ]
    },
    {
      "original": "「ん？」",
      "lines": [
        14
      ]
    },
    {
      "original": "「アルルのことを考えてた」",
      "lines": [
        35
      ]
    },
    {
      "original": "「二人一緒に土左衛門ですね」",
      "lines": [
        48
      ]
    },
    {
      "original": "「何考えてたんですか？」",
      "lines": [
        32
      ]
    },
    {
      "original": "「嬉しいです、蓮さん」",
      "lines": [
        42
      ]
    },
    {
      "original": "「少し眠くなっちゃいました」",
      "lines": [
        62
      ]
    },
    {
      "original": "「幸せで悪いことなんてないだろ？」",
      "lines": [
        54
      ]
    },
    {
      "original": "「幸せの一部です」",
      "lines": [
        60
      ]
    },
    {
      "original": "「怖いなぁ、ミイラジョーク」",
      "lines": [
        51
      ]
    },
    {
      "original": "「私が好きになったのが蓮さんで良かった、って思うんです」",
      "lines": [
        58
      ]
    },
    {
      "original": "「私のことですか？」",
      "lines": [
        36
      ]
    },
    {
      "original": "「蓮さぁん」",
      "lines": [
        19
      ]
    },
    {
      "original": "「蓮さん」",
      "lines": [
        13
      ]
    },
    {
      "original": "「話しならいつでもできるよ。それはもう、明日にでも」",
      "lines": [
        66
      ]
    },
    {
      "original": "『アルルに溺れる』を自分の泳ぎにかけてるんだろう。",
      "lines": [
        47
      ]
    },
    {
      "original": "あぁもう、なんだこの甘い空気。",
      "lines": [
        43
      ]
    },
    {
      "original": "いやもう溺れてるんだけどさ。",
      "lines": [
        30
      ]
    },
    {
      "original": "ぎゅっと体を近づけてくる。",
      "lines": [
        37
      ]
    },
    {
      "original": "このままどんどん飲まれてダメになっていきそう。",
      "lines": [
        31
      ]
    },
    {
      "original": "こんな調子だ。",
      "lines": [
        23
      ]
    },
    {
      "original": "そして何より……。",
      "lines": [
        12
      ]
    },
    {
      "original": "そして俺も目を閉じる。",
      "lines": [
        77
      ]
    },
    {
      "original": "そして少し経つと……。",
      "lines": [
        18
      ]
    },
    {
      "original": "その寝顔も、寝息も。",
      "lines": [
        72
      ]
    },
    {
      "original": "その眠りはきっと、アルルがくれたものだろう。",
      "lines": [
        80
      ]
    },
    {
      "original": "その言葉を最後に、アルルは静かな寝息を立て始めた。",
      "lines": [
        70
      ]
    },
    {
      "original": "そりゃ初めてであそこまで激しくしたらなぁ。",
      "lines": [
        63
      ]
    },
    {
      "original": "それから、何か思いついたのか、ぎゅっと俺のことを掴んだ。",
      "lines": [
        57
      ]
    },
    {
      "original": "それはこちらとしても嬉しい限りなんだけど。",
      "lines": [
        27
      ]
    },
    {
      "original": "そんな状態で、さっきからずーっとゴロゴロしている。",
      "lines": [
        11
      ]
    },
    {
      "original": "ちなみに部屋に戻ることも考えたけど……。",
      "lines": [
        24
      ]
    },
    {
      "original": "という俺もさっきから睡魔と戦ってるわけなんだけど。",
      "lines": [
        64
      ]
    },
    {
      "original": "とても幸せそうで。",
      "lines": [
        73
      ]
    },
    {
      "original": "なので動けずにずっと甘々タイム。",
      "lines": [
        26
      ]
    },
    {
      "original": "ぽっと顔が赤くなる。",
      "lines": [
        41
      ]
    },
    {
      "original": "まずい……このままアルルに溺れていきそう。",
      "lines": [
        29
      ]
    },
    {
      "original": "もっとも、もうこれ以上ないくらいくっついてるんだけど。",
      "lines": [
        38
      ]
    },
    {
      "original": "アルル",
      "lines": [
        6,
        8,
        13,
        15,
        19,
        21,
        32,
        36,
        40,
        42,
        45,
        48,
        50,
        53,
        55,
        58,
        60,
        61,
        62,
        65,
        67,
        69,
        71
      ]
    },
    {
      "original": "アルルに顔を埋めたくなる。",
      "lines": [
        44
      ]
    },
    {
      "original": "アルルは呼ぶだけ呼んで微笑み続けている。",
      "lines": [
        17
      ]
    },
    {
      "original": "ウトウトしていると、そんなふうに話しかけてきた。",
      "lines": [
        34
      ]
    },
    {
      "original": "ベッドから体を浮かした一瞬でアルルは顔を曇らせてしまった。",
      "lines": [
        25
      ]
    },
    {
      "original": "マミーに言われるとほんとシャレにならない。",
      "lines": [
        52
      ]
    },
    {
      "original": "上手い言葉が見つからないのか、ベッドの中で身体をもぞもぞさせる。",
      "lines": [
        56
      ]
    },
    {
      "original": "傍にいるアルルの温度を感じながら。",
      "lines": [
        78
      ]
    },
    {
      "original": "優しい眠りに入って行った。",
      "lines": [
        79
      ]
    },
    {
      "original": "包帯と幸せに包まれて",
      "lines": [
        0,
        83
      ]
    },
    {
      "original": "瞼が徐々に下がっていくアルルを見つめながら、俺も睡魔に身を任せる。",
      "lines": [
        68
      ]
    },
    {
      "original": "細くて綺麗な睫をそっと撫でて。",
      "lines": [
        76
      ]
    },
    {
      "original": "興奮のために寝られず、かと言って起きて何かするには疲労が濃い。",
      "lines": [
        10
      ]
    },
    {
      "original": "蓮",
      "lines": [
        7,
        14,
        16,
        20,
        22,
        28,
        33,
        35,
        39,
        46,
        49,
        51,
        54,
        59,
        66,
        75
      ]
    },
    {
      "original": "行為が終わった後、俺たちはベッドに横になっていた。",
      "lines": [
        9
      ]
    },
    {
      "original": "見てるだけで心が満たされ、落ち着いてくる。",
      "lines": [
        74
      ]
    }
